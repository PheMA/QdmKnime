						<qualifier>
							<name code="363702006" displayName="has focus"/>
							<value code="110465008" displayName="clinical trial">
								<qualifier>
									<name code="363702006" displayName="has focus"/>
									<value code="230690007" displayName="stroke"/>
								</qualifier>
							</value>
						</qualifier>
					</code>
					<participant typeCode="SBJ">
						<signatureCode code="S"/>
